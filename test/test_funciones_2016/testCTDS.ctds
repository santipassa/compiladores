// retorna el factorial de v
integer factorial (integer v){
  integer limit;
  limit = 15;
  if ( v > limit) { return -1;}
  { 
	integer c, fact;
	c = 0;
	fact = 1;
    while (c<v){
       c = c+1;
       fact = fact*c;
    }
    return fact;
  }
  return 0;
}

// retorna el factorial de v
integer factorialFor (integer v){
  integer limit;
  limit = 15;
  if ( v > limit) { return -1;}
  { 
	integer c, fact;
	c = 0;
	fact = 1;
    while (c<v){
       c = c+1;
       fact = fact*c;
    }
    return fact;
  }
  return 1;
} 

// Calcula el factorial de 0 a 14 y retorna el factorial de v
integer factorialArray (integer v){
  integer limit;
  limit = 15;
  { 
    integer i;
    integer arr_fact[15];
    i=0;
    while (i<limit){
       integer c, fact;
       c = 0;
       fact = 1;
       while (c<i){
          c = c+1;
          fact = fact*c;
       }
       arr_fact[i]=fact;  
       i=i+1;
    }
    if ( v > limit-1) return -1; 
    else return arr_fact[v];
  }
  return 1;
}

// retorna el n esimo primo
integer nthprime (integer n) {
    integer i;
    integer divs;
    i = 0;
    divs = 2;
    n = n +1;
    while ( n > 0) {
      bool divides;
      divides = false;
      i = i + 1;
      while ( (! divides) && (divs < i)) {
       if ( i % divs == 0)
         { divides = true; }
       else
         { divs = divs + 1; }
      }
      divs = 2;
      if ( ! divides)
        { n = n - 1; }
    }
    return i;
}

// calcula los primeros 100 primos y retorna el n esimo
integer nthprimeArray (integer n) {

  integer j, arr[100];
  j = 0;
    
  while (j<100){
    arr[j]= nthprime(j); 
    j=j+1;
  } 
  return arr[n-1];
}

// retorna la representacion de d en binario
integer int2bin(integer d){
   integer acum, aux, i;
   acum=0;
   i=0;

   while (true){
      if(d>1){               
        aux = d % 2 ;   // toma el ultimo digito
        { integer j;
          j=0;   
                            
          while (true){    // ubica el digito en la posicion que le corresponde
             if (j<i){
               aux = aux * 10;  
               j=j+1;
               continue;  
             } 
             else { break; }
          }
         }     			

          acum=acum +aux;  // resultado parcial 
          i=i+1;           
          d=d/2;           // toma el resto del numero decimal
          continue;  
       }
       else { break; }
      }
       { integer j;
         j=0;
                            
         while (true){         // ubica el ultimo digito en la posicion que corresponde
            if (j<i){
              d = d * 10;
              j=j+1;
              continue;  
            } 
            else { break; }
         }   	
	   }
	return acum + d;  // resultaod final y retorna
}

// retorna el maximo comun divisor de a y b,
integer gcd (integer a, integer b) {
	integer i;
	integer result;
    i=1;
    result = i;
	while ( i < (a+b)) {
		if (((a % i) == 0) &&  ((b%i) ==0 ))
			{ result = i; }
		i = i + 1;
	}
	return result;
}


// Calcula el valor del real m elevado a la n
integer potencia(integer m, integer n){
	integer i;
	integer acum;
	acum=1;
	i=1;
	while (true){
		if(i<n || i==n){
			acum=acum * m;
			i=i+1;
		}
		else { break;}
	} 
	return(acum);  
}

