class main {
  integer potencia(integer m, integer n){
	integer i;
	integer acum;
	acum=1;
	i=1;
	while (true){
		if(i<n || i==n){
			acum=acum * m;
			i=i+1;
		}
		else { break;}
	} 
	return(acum);  
}

  integer main (){
    return potencia(2,5);
  }

}

