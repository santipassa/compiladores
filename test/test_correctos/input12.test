class persona{
   integer edad;
   bool femenino;

   void persona(integer e, bool f){
       edad = e;
       femenino = f;
       return;
   }


   void persona2(){
       edad = 0;
       femenino = false; //por defecto es hombre.
      return;
   }

   integer getEdad( ){
      
      return edad ; 
   }

  
   bool esHombre(){
      if (!femenino){
          return true;
      }
      else{ 
          return false;
      }
   }

   void incrementarEdad(){
       edad += 1 ;
       return;

   }
 
}

class main{

   void printI(integer i)extern;

 
   void main(){

        persona clara,juan;
        integer y;
        y = 7;

        clara.persona(y,true);
        juan.persona2();
 
        if(clara.esHombre()){
           clara.incrementarEdad();
        }else{
          printI(clara.getEdad());
        }
        
        y = clara.edad; 
        printI(y); 
        return;
   }

}
